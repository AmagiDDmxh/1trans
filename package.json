{
  "name": "translator",
  "version": "0.0.1",
  "description": "Translate transifex translations using DeepL API",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "author": "Amagi DDmxh",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.36",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "copy-text-to-clipboard": "^3.0.1",
    "dotenv": "^8.2.0"
  }
}
