{
  "name": "translator",
  "version": "0.0.1",
  "description": "Translate transifex translations using DeepL API",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build && npm run global",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g && one-trans",
    "clean:some": "rm -rf ./dist",
    "test": "ava"
  },
  "bin": {
    "one-trans": "bin/index.js"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "author": "Amagi DDmxh",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.36",
    "@types/fs-extra": "^9.0.12",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "commander": "^8.0.0",
    "copy-text-to-clipboard": "^3.0.1",
    "dotenv": "^8.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0"
  }
}
