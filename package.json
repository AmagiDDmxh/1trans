{
  "name": "@onekey/1trans",
  "version": "0.0.3",
  "description": "Translate transifex translations using DeepL API",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build && npm run global",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g && 1trans",
    "clean:some": "rm -rf ./dist",
    "refresh": "rm -rf ./node_modules ./yarn-lock.json && yarn",
    "test": "ava"
  },
  "bin": {
    "1trans": "bin/index.js"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "author": "Amagi DDmxh",
  "homepage": "https://onekey.so/",
  "bugs": {
    "url": "https://github.com/amagiddmxh/1trans/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^14.14.36",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "commander": "^8.0.0",
    "copy-text-to-clipboard": "^3.0.1",
    "dotenv": "^8.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmagiDDmxh/1trans.git"
  },
  "keywords": [
    "translator",
    "deepl",
    "onekey",
    "cli"
  ]
}